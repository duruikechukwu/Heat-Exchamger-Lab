             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="25" d:DesignWidth="760">
    <StackPanel Margin="10,0,0,0" Orientation="Horizontal">
        <TextBox x:Name="txtTemp" Width="150" BorderBrush="Black"/>
        <TextBox x:Name="txtCp" Width="150" BorderBrush="Black"/>   
        <TextBox x:Name="txtMiu" Width="150" BorderBrush="Black"/>
        <TextBox x:Name="txtRho" Width="150" BorderBrush="Black"/>
        <TextBox x:Name="txtK" Width="150" BorderBrush="Black"/>
    </StackPanel>
</UserControl>
